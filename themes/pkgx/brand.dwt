<!DOCTYPE html>
<html lang="vi-VN">
<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
<!--[if lt IE 9]>
  <script src="{$option.static_path}/js/html5shiv.js"></script>
  <script src="{$option.static_path}/js/respond.min.js"></script>
<![endif]-->
<head>
<meta charset="utf-8">
{if $description}<meta name="description" content="{$description}"/>{/if}
{if $keywords}<meta name="keywords" content="{$keywords}"/>{/if}
<meta content="INDEX,FOLLOW" name="robots" />
<title>Phụ Kiện {$brand.brand_name} Giá Sỉ, Bán Buôn phụ kiện {$brand.brand_name} rẻ nhất</title>
<link rel="canonical" href="{$base_path}/thuong-hieu/{$brand.site_url}.html" />
<link rel="alternate" href="{$base_path}/thuong-hieu/{$brand.site_url}.html" hreflang="vi-vn" />
<meta itemprop="image" content="{$brand.brand_logo}" />
<meta property="og:image" content="{$brand.brand_logo}" />
<meta property="og:url" itemprop="url" content="{$base_path}/thuong-hieu/{$brand.site_url}.html" />
<meta property="og:image:secure_url" itemprop="thumbnailUrl" content="{$brand.brand_logo}" />
<!--#region Thông tin sản phẩm theo Cate, hãng, filter-->
<script type="application/ld+json">
{
"@context": "https://schema.org/",
"@type": "Product",
"name": "{$brand.brand_name}",
"image": ["{$brand.brand_logo}"],
"sku": "{$brand.brand_name}",
"mpn": "{$brand.brand_name}",
"description":"{$description}",
"brand": {
"@type": "Thing",
"name": ["Hoco","Remax","Baseus","Borofone","Airpods","Apple","Samsung","Oppo","Xiaomi","Realme","Vivo","Huawei"]
},
  "review": {
    "@type": "Review",
    "reviewRating": {
      "@type": "Rating",
      "ratingValue": "5",
      "bestRating": "5"
    },
    "author": {
      "@type": "Person",
      "name": "Phụ Kiện Điện Thoại Giá Sỉ"
    }
  },
  "aggregateRating": {
    "@type": "AggregateRating",
    "ratingValue": "5",
    "reviewCount": "1939"
  },
"offers": {
  "@type": "AggregateOffer",
  "url": "{$base_path}/thuong-hieu/{$brand.site_url}.html",
  "priceCurrency": "VND",
  "lowPrice": "1000",
  "highPrice": "100000",
  "price": "1000",
  "offerCount": "17563",
  "priceValidUntil": "2059-11-20",
  "itemCondition": "http://schema.org/NewCondition",
  "availability": "https://schema.org/InStock"
}
}
</script>
{include file='library/html_header_nosale.lbi'}
{insert_css files="css/brand_list.css"}
</head>
<body id="page_{$pname}">
{include file='library/page_header.lbi'}
<section>
	{include file='library/ur_here.lbi'}

	<div class="col_sub">
	{$render.before_col_sub}
		<div class="box thin_box brand_details">
			<h1 class="box_title">PHỤ KIỆN {$brand.brand_name}</h1>
			<div class="bd brand_info">
				<p class="description">
					{if $brand.brand_logo}<img src="{$brand.brand_logo}" alt="{$brand.brand_name}">{/if}
				</p>
				<ul class="cat_list">
                    {foreach from=$brand_cat_list item=cat}
                    {if $cat.goods_count != 0}
                    <li><h3><a href="{$cat.url}"{if $cat.cat_id eq $category} class="current"{/if}>{$cat.cat_name|escape:html}</a></h3></li>
                    {/if}
                    {/foreach}
                </ul>
			</div>
		</div>

	{$render.after_col_sub}
	</div>
    <div class="col_main">
    {$render.before_col_main}
    <!-- #BeginLibraryItem "/library/brand_goods_list.lbi" --><!-- #EndLibraryItem -->
    {$render.after_col_main}
    </div>
        <div class="clr"></div>
    {if $brand.brand_cat_desc}

    <article style="margin-bottom: 10px">
            <div class="article_content">
            {$brand.brand_cat_desc}
            </div>
    </article>
    {/if}
    {if $brand.brand_long_desc}
    <article style="margin-bottom: 10px">
          <div class="article_content">
        {$brand.brand_long_desc}
      </div>
    </article>{/if}
    </section>
{include file='library/page_footer.lbi'}
{insert_js name='default.Desktop.min.js' no_minify='js/jquery.min.1.8.3.js,js/plugins.js,js/lang.vi_vn.js' minify='js/global.js,js/init.js'}
{include file='library/html_footer.lbi'}
</body>
</html>
