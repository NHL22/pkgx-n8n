<!DOCTYPE html>
<html lang="vi-VN">
<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
<!--[if lt IE 9]>
  <script src="{$option.static_path}/js/html5shiv.js"></script>
  <script src="{$option.static_path}/js/respond.min.js"></script>
<![endif]-->
<head>
    <meta charset="utf-8">
    {include file='library/html_header.lbi'}
     {if $description}<meta name="description" content="{$description}"/>{/if}
    {if $keywords}<meta name="keywords" content="{$keywords}"/>{/if}
    <meta content="{$meta_robots}" name="robots" />
    <title>{$page_title}</title>
    <link rel="canonical" href="{$base_path}/" />
    <link rel="alternate" href="{$base_path}/" hreflang="vi-vn" />
    <meta property="og:url" itemprop="url" content="{$base_path}/" />
    <meta itemprop="image" content="{$base_path}/cdn/upload/files/profile/logo-300x300.png" />
    <meta property="og:image" content="{$base_path}/cdn/upload/files/profile/logo-300x300.png" />
    <meta property="og:image:alt" content="{$base_path}/cdn/upload/files/profile/logo-300x300.png" />
    <meta property="og:image:secure_url" itemprop="thumbnailUrl" content="{$base_path}/cdn/upload/files/profile/logo-300x300.png" />
    <!--Private Stylesheet -->
    {insert_css files="owl-carousel/owl.carousel.2.1.css,css/global.css,css/index.css"}
</head>
<body id="page_{$pname}">
{include file='library/page_header.lbi'}
<section>
  <div class="clr clearfix" style="margin-top: 10px;"></div>

      <!--   <div class="categories_all">
            {include file='library/category_tree.lbi'}
        </div>-->
        <div class="homebanner">
            <!-- TemplateBeginEditable name="BannerHome670x300" -->
<!-- #BeginLibraryItem "/library/ad_position.lbi" -->
{insert name=ads id=$ads_id num=$ads_num}
 <!-- #EndLibraryItem -->
<!-- TemplateEndEditable -->
        </div>
        <div class="Right-BannerHome">
          {insert name=ads id=16 num=$ads_num}
        </div>
        <div class="clr clearfix" ></div>
        <div class="hd">
        <div class="box clearfix">
        <div class="h1_home"><h1>{$des_home}</h1></div>
        <div class="img_filter"  id="filter_sub">
            {foreach from=get_categories_tree() item='cat' name='categories'}
            <a {if $active_url eq $cat.url}class="selected"{/if} href="{$cat.url}">
                {if $cat.icon}<img width="55px" height="55px" src="{$cat.icon}" alt="{$cat.name|escape}">{/if}
                <h2>{$cat.name}</h2>
            </a>
            {/foreach}
         </div>
         </div>
         <div class="box clearfix">
             <ul class="botom-bannerhome">
               <li>{insert name=ads id=23 num=$ads_num}</li>
               <li>{insert name=ads id=24 num=$ads_num}</li>
               <li>{insert name=ads id=25 num=$ads_num}</li>
               <li>{insert name=ads id=26 num=$ads_num}</li>
             </ul>
           </div>
                 <div class="clr clearfix" style="margin-top: 10px;"></div>
     <!-- TemplateBeginEditable name="Shock" -->
<!-- TemplateEndEditable -->
    <!-- TemplateBeginEditable name="Promotion" -->
<!-- TemplateEndEditable -->


     <!-- TemplateBeginEditable name="Catalog-Main" -->
<!-- #BeginLibraryItem "/library/cat_goods.lbi" -->
<div class="box clearfix">
    <div class="hd">
        <div class="box_title"><h2>{$goods_cat.name|escape:html}</h2></div>
        <div class="extra"><a href="{$goods_cat.url}">Xem tất cả</a></div>

        <ul class="box-cat-all">
            <!--{foreach from=$goods_cat.cat_clild item=cat name=foo}-->
        <li>  <h3> <a href="{$cat.url}">{$cat.name|escape}</a></h3></li>
            <!--{/foreach}-->
            </ul>
    </div>
    <div class="cat_goods clearfix" id="catalog-{$goods_cat.id}">
    	<ul class="homeproduct">
    		{foreach from=$cat_goods item=goods name=cat_goods}
    		<li>
                {if $goods.discount}<span class="rate">Giảm {$goods.discount}</span>{/if}
    			<a href="{$goods.url}" title="{$goods.name|escape:html}">
                    {if $goods.thumb_webp}
                    <picture>
                        <source width="180" height="180" srcset="{$cdn_path}/{$goods.thumb_webp}" type="image/webp" />
                        <img width="180" height="180" class="photo" src="{$cdn_path}/{$goods.thumb}" alt="{$goods.name|escape:html}"/>
                    </picture>
                    {else}
                    <img width="180" height="180" class="photo" src="{$cdn_path}/{$goods.thumb}" alt="{$goods.name|escape:html}"/>
                    {/if}
                    {if $goods.is_hot} <span class="icon_hot"></span> {/if}
                    {if $goods.goods_number eq 0} <p class="preorder">Hàng đang về</p>{/if}

                    <h3>{$goods.name|escape:html}</h3>
                    <div class="price">
                        Giá Sỉ
                         {if $goods.promote_price}
                        <strong>{$goods.promote_price}</strong><span>{$goods.shop_price}</span>
                         {else}
                         <strong>{$goods.shop_price}</strong>
                        {/if}

                    </div>
                  <!-- <p class="cprice">Giá CTV: {$goods.partner_price}</p> -->
                    <p class="cprice">Giá Lẻ: {$goods.deal_price}</p>
                   <!-- <div class="ratingresult">
                       <span class="rank rank_{$goods.comment_rank}"></span>
                       <span>{$goods.comment_count} đánh giá</span>
                   </div> -->
                    <div class="promotion clr">{$goods.seller_note}</div>
                </a>
                 <div class="form_add form_add{$goods.id}">
                    <div class="button-container">
                        <button class="qty_button cart-qty-minus" type="button" value="-">-</button>
                        <input type="text" min="1" max="10" class="qty qty{$goods.id}" step="1" name="goods_number" value="1">
                        <button class="qty_button cart-qty-plus" type="button" value="+">+</button>
                    </div>
                    <button class="quick_add" type="button" onclick="quickBuy({$goods.id})"><i class="icontgdd-cart"></i></button>
                </div>
    		</li>
    		{/foreach}
    	</ul>
    </div>
</div>

